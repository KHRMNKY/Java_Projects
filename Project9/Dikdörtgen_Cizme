// dikdörtgenin verilen iki kenar uzunluğu ile * işareti ile dikdörtgen çizimi.
package tutorial2;
import java.util.Scanner;
public class Dikdörtgen {
	public static void main(String[] args){
		Scanner sc=new Scanner(System.in);
		System.out.println("dikdörtgen x uzunluğu= ");
		int x=sc.nextInt();
		System.out.println("dikdörtgenin y uzunluk= ");
		int y=sc.nextInt();
		
		for(int i=0;i<x;i++) {
			for(int j=0;j<y;j++) {
				System.out.print("*");
			}
			System.out.println();
			
		}
		
		
	}

}
